  {
    "@id": "http://biii.eu/node/1566",
    "@type": [
      "http://biii.eu/node/software"
    ],
    "http://bise-eu.info/core-ontology#hasAuthor": [
      {
        "@value": "Steven Condamine, Dorly Verdier, Arlette Kolta. "
      }
    ],
    "http://bise-eu.info/core-ontology#hasType": [

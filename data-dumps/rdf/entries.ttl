        rdfs:label            "Hough Circles" ;
        rdfs:comment          "This plugin applies the Hough Transform for Circles to an 8-Bit image, shows the resulting Hough Space in a new window and marks the centers of the found circles." ;
        biii:hasCurator       <http://biii.org/curator/id:587> ;
        biii:hasAuthor        "Hemerson Pistori, Eduardo Rocha Costa" ;
        biii:hasLocationPath  "https://imagej.nih.gov/ij/plugins/hough-circles.html" .

